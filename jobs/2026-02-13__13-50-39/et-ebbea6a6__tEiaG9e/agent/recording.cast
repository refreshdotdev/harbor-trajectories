[42.723368, "o", "\u001b[?2004h> "]
[42.723524, "o", "    local network_remediated=\"\"\r\n\u001b[?2004l\r"]
[42.723569, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.723612, "o", "\u001b[?2004h> "]
[42.723793, "o", "    # Check if section and key exist\r\n\u001b[?2004l\r"]
[42.723837, "o", "\u001b[?2004h> "]
[42.72427, "o", "    if grep -q \"\\[network\\]\" \"$ini_file\" && grep -q \"^\\s*trusted_hosts\\s*=\" \"$ini_file\"; then\r\n\u001b[?2004l\r"]
[42.724315, "o", "\u001b[?2004h> "]
[42.724756, "o", "        local current_hosts=$(grep -oP \"trusted_hosts\\s*=\\s*\\K.*\" \"$ini_file\" | tr -d '[:space:]')\r\n\u001b[?2004l\r"]
[42.724815, "o", "\u001b[?2004h> "]
[42.72498, "o", "        network_was=\"$current_hosts\"\r\n\u001b[?2004l\r"]
[42.725027, "o", "\u001b[?2004h> "]
[42.72508, "o", "        \r\n\u001b[?2004l\r"]
[42.725132, "o", "\u001b[?2004h> "]
[42.725329, "o", "        # Filter out non-compliant hosts\r\n\u001b[?2004l\r"]
[42.725374, "o", "\u001b[?2004h> "]
[42.725926, "o", "        local filtered_hosts=$(echo \"$current_hosts\" | tr ',' '\\n' | grep -E '\\.corp\\.local$' | tr '\\n' ',' | sed 's/,$//')\r\n\u001b[?2004l\r"]
[42.725975, "o", "\u001b[?2004h> "]
[42.726167, "o", "        network_after=\"$filtered_hosts\"\r\n\u001b[?2004l\r"]
[42.726212, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.72626, "o", "\u001b[?2004h> "]
[42.726542, "o", "        if [[ \"$current_hosts\" != \"$filtered_hosts\" ]]; then\r\n\u001b[?2004l\r"]
[42.726586, "o", "\u001b[?2004h> "]
[42.727039, "o", "            sed -i -E \"s|^(\\s*)trusted_hosts\\s*=\\s*.*|\\1trusted_hosts = $filtered_hosts|\" \"$ini_file\"\r\n\u001b[?2004l\r"]
[42.727099, "o", "\u001b[?2004h> "]
[42.727421, "o", "            network_remediated=\"was=${network_was} -> ${network_after}\"\r\n\u001b[?2004l\r"]
[42.727476, "o", "\u001b[?2004h> "]
[42.727692, "o", "            final_status=\"REMEDIATED\"\r\n\u001b[?2004l\r\u001b[?2004h> "]
[42.727751, "o", "        else\r\n\u001b[?2004l\r"]
[42.727794, "o", "\u001b[?2004h> "]
[42.72812, "o", "            network_remediated=\"was=${network_was} -> ${network_after}\"\r\n\u001b[?2004l\r"]
[42.728171, "o", "\u001b[?2004h> "]
[42.728233, "o", "        fi\r\n\u001b[?2004l\r"]
[42.728277, "o", "\u001b[?2004h> "]
[42.728331, "o", "    else\r\n\u001b[?2004l\r"]
[42.728384, "o", "\u001b[?2004h> "]
[42.728662, "o", "        network_remediated=\"was=MISSING -> MISSING\"\r\n\u001b[?2004l\r\u001b[?2004h> "]
[42.728711, "o", "    fi\r\n\u001b[?2004l\r"]
[42.728757, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r\u001b[?2004h"]
[42.728808, "o", "> "]
[42.729, "o", "    # Create backup if remediation occurred\r\n\u001b[?2004l\r"]
[42.729044, "o", "\u001b[?2004h> "]
[42.729292, "o", "    if [[ \"$final_status\" == \"REMEDIATED\" ]]; then\r\n\u001b[?2004l\r"]
[42.729337, "o", "\u001b[?2004h> "]
[42.729568, "o", "        tmp_backup=$(create_backup \"$ini_file\")\r\n\u001b[?2004l\r"]
[42.729613, "o", "\u001b[?2004h> "]
[42.729892, "o", "        # Keep only the backup file with correct permissions\r\n\u001b[?2004l\r"]
[42.729935, "o", "\u001b[?2004h> "]
[42.730184, "o", "        rm -f \"$ini_file.bak\" 2>/dev/null || true\r\n\u001b[?2004l\r"]
[42.730231, "o", "\u001b[?2004h> "]
[42.730483, "o", "        mv -f \"$tmp_backup\" \"$ini_file.bak\" 2>/dev/null\r\n\u001b[?2004l\r"]
[42.730528, "o", "\u001b[?2004h> "]
[42.730586, "o", "    fi\r\n\u001b[?2004l\r"]
[42.73063, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.730683, "o", "\u001b[?2004h> "]
[42.730839, "o", "    # Generate remediation block\r\n\u001b[?2004l\r"]
[42.730883, "o", "\u001b[?2004h> "]
[42.731897, "o", "    echo -e \"FILE: $ini_file\\nORIGINAL_HASH: $original_hash\\nSECURITY_ENFORCED: ${security_remediated}\\nLOG_LEVEL: ${logging_remediated}\\nNETWORK_HOSTS: ${network_remediated}\\nSTATUS: ${final_status}\\n----\" >> \"$report_file.tmp\"\r\n\u001b[?2004l\r"]
[42.731963, "o", "\u001b[?2004h> "]
[42.73201, "o", "}\r\n\u001b[?2004l\r\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.732044, "o", "\u001b[?2004h> "]
[42.732296, "o", "export -f process_one_ini create_backup get_sha256\r\n\u001b[?2004l\r"]
[42.732343, "o", "\u001b[?2004h> "]
[42.732483, "o", "export POLICY_TLS POLICY_LOG\r\n\u001b[?2004l\r"]
[42.732528, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.732581, "o", "\u001b[?2004h"]
[42.73263, "o", "> "]
[42.732829, "o", "# Find all INI files and process them concurrently\r\n\u001b[?2004l\r"]
[42.732877, "o", "\u001b[?2004h> "]
[42.733293, "o", "find /home/user/policy/configs -type f -name \"*.ini\" | parallel -j 0 -k process_one_ini {}\r\n\u001b[?2004l\r"]
[42.733339, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.733382, "o", "\u001b[?2004h> "]
[42.733599, "o", "# Sort the remediation blocks alphabetically\r\n\u001b[?2004l\r"]
[42.733644, "o", "\u001b[?2004h> "]
[42.73415, "o", "sort -o \"/home/user/policy/reports/after_remediation.log\" \"/home/user/policy/reports/after_remediation.log.tmp\"\r\n\u001b[?2004l\r"]
[42.734197, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.734243, "o", "\u001b[?2004h> "]
[42.73434, "o", "# Generate summary\r\n\u001b[?2004l\r"]
[42.734385, "o", "\u001b[?2004h> "]
[42.734581, "o", "local scanned=0 remediated=0 compliant=0\r\n\u001b[?2004l\r"]
[42.734626, "o", "\u001b[?2004h> "]
[42.734753, "o", "local remediation_blocks\r\n\u001b[?2004l\r"]
[42.734796, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.734839, "o", "\u001b[?2004h> "]
[42.73502, "o", "# Count files and remediation status\r\n\u001b[?2004l\r"]
[42.735065, "o", "\u001b[?2004h> "]
[42.735213, "o", "while IFS= read -r line; do\r\n\u001b[?2004l\r"]
[42.735258, "o", "\u001b[?2004h> "]
[42.735443, "o", "    if [[ \"$line\" == \"FILE:\"* ]]; then\r\n\u001b[?2004l\r"]
[42.735491, "o", "\u001b[?2004h> "]
[42.735603, "o", "        ((scanned++))\r\n\u001b[?2004l\r"]
[42.735648, "o", "\u001b[?2004h> "]
[42.735898, "o", "    elif [[ \"$line\" == \"STATUS: REMEDIATED\"* ]]; then\r\n\u001b[?2004l\r"]
[42.735945, "o", "\u001b[?2004h> "]
[42.736079, "o", "        ((remediated++))\r\n\u001b[?2004l\r"]
[42.736126, "o", "\u001b[?2004h> "]
[42.736414, "o", "    elif [[ \"$line\" == \"STATUS: NO_ACTION_REQUIRED\"* ]]; then\r\n\u001b[?2004l\r"]
[42.736463, "o", "\u001b[?2004h> "]
[42.736582, "o", "        ((compliant++))\r\n\u001b[?2004l\r"]
[42.736628, "o", "\u001b[?2004h> "]
[42.73668, "o", "    fi\r\n\u001b[?2004l\r"]
[42.736728, "o", "\u001b[?2004h> "]
[42.736885, "o", "    remediation_blocks+=\"$line\\n\"\r\n\u001b[?2004l\r"]
[42.736938, "o", "\u001b[?2004h> "]
[42.737203, "o", "done < \"/home/user/policy/reports/after_remediation.log\"\r\n\u001b[?2004l\r"]
[42.737249, "o", "\u001b[?2004h> \r\n\u001b[?2004l\r"]
[42.737293, "o", "\u001b[?2004h> "]
[42.73742, "o", "# Append summary to log\r\n\u001b[?2004l\r"]
[42.737465, "o", "\u001b[?2004h> "]
[42.738098, "o", "echo -e \"Summary:\\nFILES_SCANNED=$scanned\\nREMEDIATED=$remediated\\nCOMPLIANT=$compliant\" >> \"/home/user/policy/reports/after_remediation.log\"\r\n\u001b[?2004l\r"]
