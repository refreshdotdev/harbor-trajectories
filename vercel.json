{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd harbor/viewer && npm install && npm run build",
  "functions": {
    "api/index.py": {
      "runtime": "python3.12",
      "maxDuration": 60,
      "includeFiles": "{jobs/**,harbor/src/**,harbor/viewer/build/client/**}"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
